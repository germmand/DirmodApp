{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier", "jest"],
    "rules": {
        "prettier/prettier": ["error"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-param-reassign": [2, { "props": false }],
    },
    "env": {
        "jest/globals": true,
        "browser": true,
        "node": true,
    },
}
